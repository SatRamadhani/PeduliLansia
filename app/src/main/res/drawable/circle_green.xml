<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Ini pisah kawan. -->
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
            <stroke
                android:color="@color/primary_green"
                android:width="1dp" />
            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:color="@color/primary_green"
                android:dashGap="8dp"
                android:dashWidth="8dp"
                android:width="1dp" />
            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>
    </item>
</layer-list>